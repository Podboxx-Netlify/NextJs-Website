{
  "name": "demo-podcast-podboxx",
  "version": "0.1.0",
  "private": true,
  "nyc": {
    "report-dir": "cypress-coverage"
  },
  "scripts": {
    "dev": "next dev -p 5000",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "next lint",
    "test": "yarn test:jest --config ./jest.config.ts && yarn test:cy",
    "test:cy": "cypress run",
    "test:jest": "jest --config ./jest.config.ts",
    "copy:reports": "cp cypress-coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "yarn copy:reports && mkdir .nyc_output || true",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn combine:reports",
    "report:combined": "npx nyc report --reporter lcov --reporter text --report-dir coverage",
    "cy:open": "cypress open",
    "cypress:open": "cypress open",
    "coverage": "nyc check-coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@netlify/plugin-nextjs": "^3.5.0",
    "@sentry/nextjs": "^6.10.0",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "braintree-web": "^3.79.1",
    "clsx": "^1.1.1",
    "daisyui": "^1.10.0",
    "dayjs": "^1.10.6",
    "j-tockauth": "^1.2.7",
    "js-cookie": "^3.0.0",
    "next": "11.0.1",
    "next-seo": "^4.25.1",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-paginate": "^7.1.3",
    "react-toastify": "^7.0.4",
    "sass": "^1.35.1",
    "swr": "^0.5.6",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@cypress/code-coverage": "^3.9.10",
    "@types/jest": "^26.0.24",
    "@types/jquery": "^3.5.5",
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.11",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cypress": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "jest": "^27.0.6",
    "postcss": "^8.3.5",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^2.2.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
